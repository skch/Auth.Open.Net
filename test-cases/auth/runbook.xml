<?xml version="1.0" encoding="utf-8" ?>
<runbook>
  <header>
  </header>
  <variables>
	<!--<var id="user" value="" /> -->
  </variables>
  <sequence id="OAuth2" showContext="yes">  	    
	<context>{"user": {} }</context>
	<test src="register-user" save="user"/>
	<test src="auth-pass" save="var1" disable="yes">
		<header>
			<header id="token" value="{{access_token}}" />
		</header>
	</test>
	<test src="user-info" disable="yes"/> 
  </sequence>
</runbook>
