<?xml version="1.0" encoding="utf-8" ?>
<rest type="POST" url="{{base}}/api/Authenticate/UserInfo">
  <data>
	{
	    "client_id": "{{client}}",
		"response_type": "code",
		"access_token":"{{var1/access_token}}"
	}
  </data>
  <result code="200" type="application/json; charset=utf-8">
   {
		"type": "object",
		"properties": {
		  "ID": { "type": "integer" },
		  "FirstName": { "type": "string" },
		  "LastName": { "type": "string" },
		  "Prefix": { "type": "string" },
		  "Position": { "type": "string" }
		},
		"required": ["ID", "FirstName", "LastName", "Prefix", "Position"]
    }
  </result>
</rest>
